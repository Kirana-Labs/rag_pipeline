# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/rag_db

# GPU Configuration
USE_GPU=true

# Embedding Configuration
EMBEDDING_PROVIDER=voyage  # or "sentence-transformers"
EMBEDDING_MODEL=voyage-context-3  # or "all-MiniLM-L6-v2" for sentence-transformers
EMBEDDING_DIMENSIONS=512  # 256, 512, 1024, 2048 for Voyage AI

# Voyage AI API Configuration
VOYAGE_API_KEY=your_voyage_api_key_here

# Reranking Configuration
USE_RERANKER=true
RERANKER_MODEL=rerank-2.5  # or "rerank-2.5-lite"

# Chunking Configuration
CHUNK_SIZE=1000
CHUNK_OVERLAP=200

# Bulk Ingestion Configuration (not used in serverless)
MAX_CONCURRENT_BULK_JOBS=3
MAX_CONCURRENT_DOCS_PER_JOB=5